-- #desc Map a pure (addition) function over a list
-- #return [[2], [3], [4]]
interface State X = get : X
	          | put : X -> Unit

map : {a -> b} -> List a -> List b
map f nil = nil
map f (cons x xs) = cons (f x) (map f xs)

main : []List (List Int)
main! = map {x -> cons (x+1) nil} (cons 1 (cons 2 (cons 3 nil)))
